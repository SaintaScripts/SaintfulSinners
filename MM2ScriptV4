local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
 local Window = OrionLib:MakeWindow({Name = "üéÖüèª SaintaScripts | Murder Mystery 2 üî™", HidePremium = false, IntroText = "SaintaScripts", SaveConfig = true, ConfigFolder = "OrionTest"})
 
